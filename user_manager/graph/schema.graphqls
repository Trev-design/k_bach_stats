type Contact {
  id: ID!
  name: String!
  email: String!
  imageFilePath: String!
}

type Profile {
  id: ID!
  bio: String!
  contact: Contact!
}

type User {
  id: ID!
  entity: String!
  profile: Profile!
}

type UserEntity {
  user: ID!
}

type Query {
  userID(entity: String!): UserEntity!
  userByID(userID: String!): User!
}
